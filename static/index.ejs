<!DOCTYPE html>
<html>

<head>
    <title>Video Chat</title>
    <link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.css" /> -->
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link href="style.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="login">
        <div id="loginWrap" class="wraps">
            <h1>Login</h1>
            <form id="loginForm">
                <div>
                    <input type="email" id="email" name="email" placeholder="Email" required />
                </div>
                <div>
                    <input type="password" id="password" name="password" placeholder="Password" required />
                </div>
                <div>
                    <input type="submit" value="Login" class="btn btn-primary btn-block btn-large" />
                    <a href="/users/register" class="btn btn-primary btn-block btn-large">Register</a>
                </div>
                <p id="loginMsg"></p>
            </form>
        </div>
    </div>
    <!-- <div> -->
    <div id="addcontact" class="wraps2 topcorner6 parent">
        <!-- <h2 style="color: white;" class="center">Add Contact</h2> -->
        <div class="topcorner2"><img src="../cross.png" id="addclose" style="height: 16px;width: 16px; -webkit-filter: invert(100%); filter: invert(100%);"></div>
        <form id="contactform" style="width: 70%;" class="center2" style="margin-top: 10px;">
            <div>
                <input type="text" id="contactname" name="contactname" placeholder="Name" required />
            </div>
            <div>
                <input type="email" id="contactemail" name="contactemail" placeholder="Email" required />
            </div>
            <div>
                <select name="cars" size="1" id="type">
                        <option value="buissness" selected>Buissness</option>
                        <option value="relative">Relative</option>
                        <option value="friend">Friend</option>
                        <!-- <option value="audi">Audi</option> -->
                      </select>
                <!-- <input type="text" id="type" name="type" placeholder="Category" required /> -->
            </div>
            <div>
                <input type="submit" value="Add" class="btn btn-primary btn-block btn-large" />
            </div>
        </form>
    </div>

    <div id="chatWrap" class="wraps">
        <h2 class="topcorner1" id="personName">Profile</h2>
        <h2 id="list" class="topcorner3"> List </h2>
        <h2 id="add" class="topcorner4"> Add </h2>
        <div class="column2" style="margin-top: 5px;"> Online Users</div>
        <div id="personImage"></div>
        <br/> <br/>
        <div id="userList">
        </div>
    </div>
    <div id="listWrap" class="wraps parent">
        <!-- <div class="column2" style="margin-top: 5px;"> Personal Contacts</div> -->
        <div id="food-table" class="row">
            <div class="column1 clicklist" id="buissnesslist">Buissness<img id="buissnessline" src="../line.png" style="height: 4px;width: 100%; -webkit-filter: invert(100%); filter: invert(100%); margin-top: 0px;" /></div>
            <div class="column clicklist" id="relativeslist">Relatives<img id="relativeline" src="../line.png" style="height: 4px;width: 100%; -webkit-filter: invert(100%); filter: invert(100%); margin-top: 0px;" /></div>
            <div class="column clicklist" id="friendslist">Friends<img id="friendline" src="../line.png" style="height : 4px;width:100%; -webkit-filter: invert(100%); filter: invert(100%); margin-top: 0px;" /></div>
            <div class="topcorner2"><img src="../cross.png" id="listclose" style="height: 16px;width: 16px; -webkit-filter: invert(100%); filter: invert(100%);"></div>
        </div>
        <div id="contacts">
        </div>
    </div>
    </div>
    <!-- <div class="topcorner2" id="list"><img src="../cross.png"></div> -->
    <!-- <div id="listWrap" class="wraps parent">
        <div class="topcorner2"><img src="../cross.png" id="listclose" style="height: 16px;width: 16px; -webkit-filter: invert(100%); filter: invert(100%);"></div>
    </div> -->
    <div id="profile4" class="wraps2 parent topcorner8">
        <div class="topcorner2"><img src="../cross.png" id="cross" style="height: 16px;width: 16px; -webkit-filter: invert(100%); filter: invert(100%);"></div>
        <div class="center">
            <img id="userimage" class="profile">
        </div>
        <br/>
        <div id="username" class="center profiletext">
        </div>
        <div id="useremail" class="center profiletext">
        </div>
        <div id="update" class="center profiletext">Update Profile</div>
    </div>
    <div id="updateprofile" class="wraps1 parent topcorner8">
        <h1 style="color: white;">Profile</h1>
        <form enctype='multipart/form-data' id="updateform" style="width: 70%;" class="center">
            <div>
                <input type="text" id="updatename" name="name" placeholder="Name" required />
            </div>
            <div>
                <input type="email" id="updateemail" name="email" placeholder="Email" required />
            </div>
            <div>
                <input type="password" id="updatepassword" name="password" placeholder="Password" required />
            </div>
            <div>
                <input type="password" id="updatepassword2" name="password2" placeholder="Confirm Password" required />
            </div>
            <input type="hidden" id="prevemail" name="prevemail" value="prevemail" />
            <input type="hidden" id="prevname" name="prevname" value="prevname" />
            <input type="hidden" id="previmage" name="previmage" value="previmage" />
            <input type="hidden" id="previd" name="previd" value="previd" />
            <div>
                <input type="file" id="profileImage" name="profileImage" required/>
            </div>
            <div>
                <input type="submit" value="Update Profile" class="btn btn-primary btn-block btn-large" />
            </div>
        </form>
        <div style="width: 70%;" class="center">
            <button id="cancel" class="btn btn-primary btn-block btn-large"> Cancel </button>
        </div>
    </div>
    <div id="video-chat" align="center">
        <h3>Video Chat</h3>
        <video id="remoteScreen" autoplay style="height:60%;width:50%;margin:10px;border-radius: 16px;">
				</video>
        <video id="localScreen" autoplay style="height:20%;width:22%;margin:10px;border-radius: 4px;">
				</video>
        <br />
        <div class="column3">
            <p id="callStatus">Calling...Waiting for the remote user's response.</p>
            <br />
            <button id="cancelCall" class="answer red">HangUp</button>
        </div>
    </div>
    <div class="callRequest">
        <div style="margin: 16px;" class="column1">You have a call from <span id="caller"></span> . Do you want to accept?</div>
        </br>
        <button class="answer green">Accept</button>
        <button class="answer red">Reject</button>
    </div>
    <div id="cover"></div>
    <script type="text/javascript" src="jquerymain.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="socket.management.js"></script>
    <!-- </div> -->
</body>

</html>